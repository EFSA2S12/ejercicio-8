 clase  pública Cubo {

     entrada del escáner estático =  nuevo  escáner ( sistema . en);

    public  static  void  main ( String [] args ) {
        int cubo, numero;

        Sistema . a cabo . println ( " Ingresa tu número: " );
        cubo = entrada . nextInt ();

        numero = ( int ) Matemáticas . pow (cubo, ( doble ) 1.0  / 3.0 );

        if (numero * numero * numero == cubo) {
            Sistema . a cabo . print ( " El número SI es un cubo \ n " );
        } más {
            Sistema . a cabo . print ( " El número NO es un cubo \ n " );
        }
    }  
}
